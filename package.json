{
    "type": "module",
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@babel/register": "^7.28.3",
        "@eslint/js": "^9.37.0",
        "eslint": "^9.37.0",
        "globals": "^16.4.0",
        "nyc": "^17.1.0",
        "qunit": "^2.24.2"
    },
    "scripts": {
        "test": "nyc qunit \"website/static/js/test/**/*.js\""
    },
    "nyc": {
        "require": [
            "@babel/register"
        ],
        "babel": true,
        "reporter": [
            "text",
            "html"
        ],
        "include": [
            "website/static/js/src/**/*.js"
        ],
        "exclude": [
            "website/static/js/test/**/*.js",
            "website/static/js/src/agendar.js",
            "website/static/js/src/index.js",
            "website/static/js/src/sala.js"
        ],
        "all": true,
        "extension": [
            ".js"
        ]
    }
}
